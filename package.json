{
  "name": "deepseek-vscode",
  "displayName": "deepseek-vscode",
  "description": "deepseek-vscode",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.97.2"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "deepseek-vscode.helloWorld",
